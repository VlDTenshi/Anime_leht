{
  "openapi": "3.0.2",
  "info": {
      "title": "AnimeAPI",
      "version": "1.0.0",
      "description": "Anime_leht"
  },
  "paths": {
      "/Animes": {
          "get": {
              "tags": [
                  "Animes"
              ],
              "responses": {
                  "200": {
                      "content": {
                          "application/json": {
                              "schema": {
                                  "type": "array",
                                  "items": {
                                      "type": "string"
                                  }
                              },
                              "examples": {
                                  "List of animes": {
                                      "value": [
                                          "Kiseiju Seino Kakurisu",
                                          "Kimi no todoke",
                                          "Akame ga Kilu",
                                          "Death note"
                                      ]
                                  }
                              }
                          }
                      }
                  }
              },
              "description": "Returns a list of all animes in API"
          }
      }
  }
}